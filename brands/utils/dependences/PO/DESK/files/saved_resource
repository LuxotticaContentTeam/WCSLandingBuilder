						$smcT5.CustomScripts.TagCode = function() {
    (function BasketScanner(){
        var getATC = setInterval(function(){        
            var img = document.querySelector('.minicart-info .minicart-info-img img');
            var title = document.querySelector('.minicart-info .minicart-info-decription h5');
            var price = document.querySelector('.minicart-info .minicart-info-decription h6 + div + div');
            if (img && title && price){
                var obj = {};
                obj.img = img.src;
                obj.title = title.textContent;
                obj.price = price.textContent;
                var itemEncoded = btoa(encodeURIComponent(JSON.stringify(obj)));
                $smcT5.CookieManager.create('smc_dyn_item', itemEncoded, 0);
            }
        }, 400);
    })();
    
    (function cartButtonListener(){
        $smcT5.CustomScripts.scanViewedItem();
    })();
};

/**
 * D.P - 2020/11/11 - Category Page DK
 */
$smcT5.CustomScripts.OnCategoryPage = function() {
    return window.pageType && window.pageType === "category" ? true : false;
}

/**
 * D.P - 2020/11/11 - Product Page DK
 */
$smcT5.CustomScripts.OnProductPage = function() {
    return window.pageType && window.pageType === "product" ? true : false;
}
$smcT5.CustomScripts.SalePageVisited = function(){
    var url = window.location.href;
    var cookie = $smcT5.CookieManager.read('smc_sale_visited');// The value in the '' needs to match with value in the create one
    if (url.indexOf('/uk/promotions-discount-50-off') > -1 && !cookie){ //whatever you put in the '' is what it will look for in the url, same as what you do with url white/blacklists
        $smcT5.CookieManager.create('smc_sale_visited', true); //This needs to match with the read one
    }
    return cookie ? true : false;
}

$smcT5.CustomScripts.DynamicItemExist = function(){
    var cookie = $smcT5.CookieManager.read('smc_dyn_item');
    return cookie ? true : false;
};

// K.S - 18-11-2022 - Scan the products when user is on product page
$smcT5.CustomScripts.scanViewedItem = function(){   
    
    // Get the viewed item cookie if it is already exist
    const cookie = $smcT5.CookieManager.read('smc_viewed_items');
    
    // Get product selectors from product page
    const imageEl = document.querySelector('.product .slick-active img');
    const titleEl = document.querySelector('#pdp .sgh-pdp__brand-name');
    const priceEl = document.querySelector('#pdp #price');
    const skuEl = window.location.href;
    const link = window.location.href;

    if(!imageEl || !titleEl || !priceEl || !skuEl) return;

    const image = imageEl.src;
    const title = titleEl.innerText.trim();
    const price = priceEl.textContent.trim();
    const sku = skuEl;
    
    // Check if there is any item was viewed before
    if(cookie){
        const item = JSON.parse(cookie);
        
        // Avoid pushing the item if it is same product        
        const findItem = item.map((el) => el.sku === sku).every((bool) => bool === false);
        if(!findItem) return;
        
        // Add product to the array if already not exists        
        item.push({ image: image, name: title, price: price, sku: sku, link: link });       
        
        const itemEncoded = item.length >= 2 ? JSON.stringify(item.slice(Math.max(item.length - 2, 0))) : JSON.stringify(item);
        $smcT5.CookieManager.create('smc_viewed_items', itemEncoded, 0);
        return;
    }
    
    // Product array
    const productArr = [];
    
    // Push item to the array if user is on product page but have not viewed any item before
    if(!cookie){ 
        productArr.push({ image: image, name: title, price: price, sku: sku, link: link });
       
        const itemEncoded = JSON.stringify(productArr);
        $smcT5.CookieManager.create('smc_viewed_items', itemEncoded, 0);
    }
};

$smcT5.CustomScripts.RecominderItemExist = function(){
    const cookie = $smcT5.CookieManager.read('smc_viewed_items');
    return cookie && JSON.parse(cookie).length >= 2 ? true : false;
};

$smcT5.CustomScripts.Country = function() {
    const country = sessionStorage.getItem('tealium_data_page_countryLast');
    return country ? country : null;
};
